{
  "scripts": {
    "production": "(npm run prod-client&) && (npm run prod-auth&) && (npm run prod-database&) && (npm run prod-docs&)",
    "prod-docs": "node ./server/docs.js",
    "prod-client": "node ./server/client.js",
    "prod-auth": "node ./server/auth.js",
    "prod-database": "node ./server/database.js",
    "docs": "nodemon ./server/docs.js",
    "client": "nodemon ./server/client.js",
    "auth": "nodemon ./server/auth.js",
    "database": "nodemon ./server/database.js",
    "server": "(npm run client&) && (npm run auth&) && (npm run database&) && (npm run docs&)",
    "start": "npm run build && npm run production",
    "dev": "npm run build && npm run server",
    "build": "npm install --omit=dev",
    "close": "killall -9 node",
    "rm-branches": "git branch | grep -v 'main' | xargs git branch -D",
    "test": "jest"
  },
  "devDependencies": {
    "jest": "^29.3.1",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "@noble/ed25519": "^1.7.1",
    "cbor": "^8.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "jsrsasign": "^10.6.1",
    "jwk-to-pem": "^2.0.5",
    "lodash": "^4.17.21",
    "nano": "^10.1.0",
    "nodemailer": "^6.8.0"
  }
}
